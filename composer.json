    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.huanqiu.com"
    },
    {
      "type": "git",
      "url": "https://gitlab.zlibs.com/xiayang/apu-sdk.git"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "symfony/console": "^4.0",
    "monolog/monolog": "^1.23",
    "aliyuncs/oss-sdk-php": "^2.3",
    "illuminate/database": "^5.6",
    "hq-platform/apu-sdk-php": "^1.0"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "App\\": "console/"
    }
  },
  "scripts": {
    "post-root-package-install": [
    ],
    "post-install-cmd": [
      "composer dump-autoload -o"
    ],
    "post-update-cmd": [
      "composer dump-autoload -o"
    ]
  },
  "config": {
    "secure-http": false
  }
}
